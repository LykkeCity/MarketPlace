@using Common
@model BackOffice.Areas.EventLogs.Models.EvntLogsRegsIndexViewModel

@{
    Layout = null;
}
<form id="frmRegData">

    <table style="margin: auto">
        <tr>

            <td>
                <input id="from" name="From" type="date" class="form-control" value="@Model.From.ToIsoDate()" />
            </td>
            <td>
                <span>-</span>
            </td>
            <td>
                <input id="to" name="To" type="date" class="form-control" value="@Model.To.ToIsoDate()" />
            </td>


            <td>
                <a class="btn btn-xs btn-success"
                        onclick="requests.doRequest({url:'@Url.Action("GetData")', formId:'#frmRegData', divResult:'#regData'});">
                    <span class="glyphicon glyphicon-refresh" aria-hidden="true"></span>
                </a>
            </td>
        </tr>
    </table>
</form>

<div id="regData">
    
</div>